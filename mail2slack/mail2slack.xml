<?xml version="1.0"?>
<Container version="2">
  <Name>mail2slack</Name>
  <Repository>stefanknaak/mail2slack:latest</Repository>
  <Registry>https://hub.docker.com/r/stefanknaak/mail2slack</Registry>
  <TemplateURL>https://raw.githubusercontent.com/corgan2222/unraid-templates/main/mail2slack/mail2slack.xml</TemplateURL>
  <Network>bridge</Network>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support>https://forums.unraid.net/topic/114590-support-corgans-docker-repository-double-take-facebox-tagbox-videobox/</Support>
  <Project>https://github.com/corgan2222/mail2slack</Project>
  <Overview>Scans a IMAP Inbox and post every Mail to a Slack Channel</Overview>
  <Category>Cloud: Productivity: Tools:</Category>
  <WebUI></WebUI>
  <TemplateURL/>
  <Icon>https://github.com/corgan2222/unraid-templates/blob/f1d341af2746fc9b20080dc5847d03b3a8647111/img/avatar.png</Icon>
  <ExtraParams>--restart unless-stopped  </ExtraParams>
  <PostArgs> </PostArgs>
  <CPUset/>
   <DonateText/>
  <DonateLink/>
  <Description>Simple container which scans a IMAP Inbox and post every Mail to a Slack Channel.
  </Description>
  <Networking>
    <Mode>bridge</Mode>
  </Networking>
  <Data/>
  <Environment>
    <Variable>
      <Value>https://hooks.slack.com/services/xxxxxxxxxxxxxxxxxx</Value>
      <Name>SLACK_END_POINT</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>Mailparser</Value>
      <Name>SLACK_SLACK_SENDER</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>https://github.com/corgan2222/unraid-templates/blob/f1d341af2746fc9b20080dc5847d03b3a8647111/img/avatar.png</Value>
      <Name>SLACK_ICON_URL</Name>
      <Mode/>
    </Variable>
  <Variable>
    <Value>FALLBACK_MESSAGE</Value>
    <Name>SLACK_SLACK_FALLBACK</Name>
    <Mode/>
  </Variable>
  <Variable>
    <Value>xxxx</Value>
    <Name>SLACK_CHANNEL</Name>
    <Mode/>
  </Variable>
  <Variable>
    <Value>xoxp-xxxxxxxx-xxxxxxxxxxx-xxxxxxxxxx-xxxxxxxxxxxxxxxxxxxxxx</Value>
    <Name>SLACK_TOKEN</Name>
    <Mode/>
  </Variable>
  <Variable>
    <Value>mail.server.com</Value>
    <Name>MAIL_MAILSERVER</Name>
    <Mode/>
  </Variable>
  <Variable>
    <Value>mail@server.com</Value>
    <Name>MAIL_MAIL_LOGIN</Name>
    <Mode/>
  </Variable>
  <Variable>
    <Value>xxxyyyyy</Value>
    <Name>MAIL_MAIL_PW</Name>
    <Mode/>
  </Variable>
  <Variable>
    <Value>Inbox</Value>
    <Name>MAIL_FOLDER</Name>
    <Mode/>
  </Variable>
  <Variable>
    <Value>https://github.com/corgan2222/mail2slack</Value>
    <Name>MAIL_AUTHOR_LINK</Name>
    <Mode/>
  </Variable>
  <Variable>
    <Value>https://github.com/corgan2222/mail2slack</Value>
    <Name>MAIL_TITLE_LINK</Name>
    <Mode/>
  </Variable>
  <Variable>
    <Value>xxx@yyyy.com</Value>
    <Name>MAIL_FOOTER</Name>
    <Mode/>
  </Variable>
  <Variable>
    <Value>https://github.com/corgan2222/unraid-templates/blob/f1d341af2746fc9b20080dc5847d03b3a8647111/img/avatar.png</Value>
    <Name>MAIL_FOOTER_ICON</Name>
    <Mode/>
  </Variable>
  <Variable>
    <Value>INFO</Value>
    <Name>GENERAL_LOG_LEVEL</Name>
    <Mode/>
  </Variable>

  </Environment>
  <Labels/>
</Container>